{
  "expo": {
    "name": "NTV solar",
    "slug": "ntv_solar",
    "version": "1.3.4",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#f47321"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ntv.isolar",
      "googleServicesFile": "./GoogleService-Info.plist",
      "buildNumber": "1.3.4",
      "requireFullScreen": true
    },
    "android": {
      "package": "com.ntv.isolar",
      "versionCode": 1003004,
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#f47321"
      },
      "permissions": []
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "22847840-3e12-4676-b520-23adf6fcd34b"
      }
    },
    "plugins": [
      "expo-font",
      "expo-localization"
    ]
  }
}
